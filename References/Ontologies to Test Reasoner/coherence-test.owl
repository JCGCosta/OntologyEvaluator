<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/unsat#"
     xml:base="http://example.org/unsat"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Ontology rdf:about="http://example.org/unsat"/>

    <!-- Classes -->
    <owl:Class rdf:about="#Cat"/>
    <owl:Class rdf:about="#Dog"/>

    <!-- Disjointness -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Cat"/>
            <rdf:Description rdf:about="#Dog"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- CatDog is a subclass of both -->
    <owl:Class rdf:about="#CatDog">
        <rdfs:subClassOf rdf:resource="#Cat"/>
        <rdfs:subClassOf rdf:resource="#Dog"/>
    </owl:Class>

</rdf:RDF>
