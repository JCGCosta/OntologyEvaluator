<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/test#"
     xml:base="http://example.org/test"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Ontology rdf:about="http://example.org/test"/>

    <!-- Classes -->
    <owl:Class rdf:about="#Male"/>
    <owl:Class rdf:about="#Female"/>

    <!-- Disjoint Classes -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Male"/>
            <rdf:Description rdf:about="#Female"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Individual in conflicting classes -->
    <owl:NamedIndividual rdf:about="#Alex">
        <rdf:type rdf:resource="#Male"/>
        <rdf:type rdf:resource="#Female"/>
    </owl:NamedIndividual>
</rdf:RDF>
